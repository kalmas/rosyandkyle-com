{
  "name": "rosyandkyle-com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "jest",
    "sync": "aws s3 sync public s3://rosyandkyle.com/ --delete",
    "uncache": "aws cloudfront create-invalidation --distribution-id E1GFE6SI71Q2TE --paths='/*' > invalidation.log",
    "deploy": "npm run build && npm run sync && npm run uncache"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "jest": "^27.4.7",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.44.3",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "eslint": "^8.5.0",
    "sirv-cli": "^1.0.0",
    "svelte-routing": "^1.6.0",
    "sveltestrap": "^5.6.3",
    "uuid": "^8.3.2"
  }
}
